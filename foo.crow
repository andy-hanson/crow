no-std
import
	crow/bool: true
	crow/col/array: +, array, new, subscript
	crow/col/collection: subscript
	crow/exception: describe, exception
	crow/log: error, info, log
	crow/misc: void
	crow/number: nat
	crow/option: new, option
	crow/string: interpolate, string, to

my-exception record(message string) exception variant-member
describe string(a my-exception)
	a.message

main void() unsafe
	x string = "foo"
	xs string array = x,
	info log xs[0]
