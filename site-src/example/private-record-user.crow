import
	crow/io/print: out, print
	./private-record

main nat^(_ string[]) summon
	r r = ()
	out print r.public
	r.public-mutable := "updated {r public-mutable}"
	out print r.public-mutable
	r call-private-setters
	out print r.public-readonly
	# won't compile:
	#r.private-mutable := "updated"

	r2 r2 = ()
	out print r2.r2-field
	# won't compile:
	#s r2 = "construct-me",

	()
