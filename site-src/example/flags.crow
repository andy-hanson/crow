import
	crow/io/print: out, print

main nat^(_ string[]) summon
	x = shoes
	out print "{x flags-to-string} receives service? {x receives-service}"
	y = x | shirt
	out print "{y flags-to-string} receives service? {y receives-service}"
	out print "negate: {~y flags-to-string}"
	out print "take off shoes: {y & ~shoes flags-to-string}"
	()

clothes flags
	shoes
	shirt
	hat

receives-service bool(a clothes)
	shoes | shirt is-in a
