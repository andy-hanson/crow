import
	crow/io/print: print

main fut nat(_ str[]) summon
	alice = all & ~hat
	print: alice flags-to-str
	print: alice.naked to-str
	print: alice.service to-str
	0 resolved

clothed flags
	shoes
	pants
	shirt
	hat

naked bool(a clothed)
	a == empty

service bool(a clothed)
	(shoes | shirt) is-in a
