import
	crow/io/print: print

main fut nat(_ str[]) summon
	x = shoes
	print: "{x flags-to-str} receives service? {x receives-service}"
	y = x | shirt
	print: "{y flags-to-str} receives service? {y receives-service}"
	print: "negate: {~y flags-to-str}"
	print: "take off shoes: {y & ~shoes flags-to-str}"
	()

clothes flags
	shoes
	shirt
	hat

receives-service bool(a clothes)
	shoes | shirt is-in a
