import
	crow/io/print: out, print

main nat^(_ string[]) summon
	x = 1 plus one
	out print "x: {x}"
	y = "one" plus one
	out print "y: {y}"
	# won't compile:
	# z = one plus 1
	z nat = one plus one
	out print "z: {z}"
	()

plus nat(a nat, b nat)
	a + b

plus string(a string, b string)
	"{a}{b}"

one nat()
	1
one string()
	"one"
