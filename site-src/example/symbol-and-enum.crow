import
	crow/io/print: out, print

main nat^(_ string[]) summon
	if piece ?= "bishop" to-chess-piece
		out print "bishop as chess-piece is {piece to-nat32}"
	if face ?= queen::chess-piece to-face-card
		out print "queen as face-card is {face to-nat32}"
	()

to-chess-piece chess-piece?(a symbol)
	a to-enum

to-face-card face-card?(a symbol)
	a to-enum

to-face-card face-card?(a chess-piece)
	a.to-symbol to-face-card

chess-piece enum
	pawn
	rook
	knight
	bishop
	queen
	king

face-card enum
	jack
	queen
	king
