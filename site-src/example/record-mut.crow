import
	crow/io/print: out, print

main fut nat(_ arr str) summon
	a bird = "crow", "cap"
	b bird = "seagull", "stetson"
	a steal-hat b
	out print a.to-str
	out print b.to-str
	()

bird record mut
	species str
	hat mut str

steal-hat void(a bird, b bird)
	a.hat := "{a hat} and {b hat}"
	b.hat := "missing hat"

to-str str(a bird)
	"{a species} in a {a hat}"
