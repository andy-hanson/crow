import
	crow/io/print: out, print

main nat^(_ string[]) summon, trusted
	# "crow" new "duck"
	xs string[] = "crow", "duck"
	# xs subscript 0
	out print xs[0]

	# "crow" new
	xs2 string[] = "goose",
	out print xs2[0]

	# new
	ys string mut[] = ()
	ys ~= "duck"
	ys ~= "goose"
	# ys set-deref 0, "crow"
	ys[0] := "crow"
	# ys set-last crow
	ys.last := "crow"
	# ys subscript 1 print
	out print ys[1]

	()
