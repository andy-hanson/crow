import
	crow/io/print: out, print
	crow/math/vec: +, new, to-string, vec2-64

main nat^(_ string[]) summon
	nats nat[] = 0, 1
	vecs vec2-64[] = (0, 1), (2, 3)
	out print "nats sum: {nats sum}"
	out print "vecs sum: {vecs sum}"
	()

add<a> spec
	new a()
	+ a(a a, b a)

sum<a> a(xs a[]) add<a>
	acc mut a = ()
	for x : xs
		acc +:= x
	acc
