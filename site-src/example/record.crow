import
	crow/io/print: out, print

main fut nat(_ arr str) summon
	a bird = "crow", "cap"
	b bird = "seagull", "stetson"
	out print a.to-str
	out print b.to-str
	out print (a mix b).to-str
	()

bird record
	species str
	hat str

mix bird(a bird, b bird)
	a.species, b.hat

to-str str(a bird)
	"{a species} in a {a hat}"
