module record
	path str
	doc-comment str
	exports list module-export

module-export record
	name sym
	doc-comment str
	type-params list type-param
	value module-export-value

module-export-value union
	alias-decl alias-decl
	builtin-decl
	enum-decl enum-decl
	extern-ptr-decl
	flags-decl flags-decl
	fun-decl fun-decl
	record-decl record-decl
	spec-decl spec-decl
	union-decl union-decl

alias-decl record
	target type-ref

enum-decl record
	members list sym

flags-decl record
	members list sym

fun-decl record
	return-type type-ref
	variadic bool
	params list param
	specs list spec-ref

param record
	name sym
	type type-ref

type-param record
	name sym

record-decl record
	purity purity
	has-private-fields bool
	fields list record-field

record-field record
	name sym
	type type-ref
	is-mut bool

spec-decl record
	body_ spec-body

spec-body union
	spec-builtin
	spec-sigs list spec-sig

spec-sig record
	comment str
	name sym
	return-type type-ref
	params list param

union-decl record
	purity purity
	members list union-member

union-member record
	name sym
	type type-ref?

type-ref record
	name sym
	type-args list type-ref

spec-ref record
	name sym
	type-args list type-ref

purity enum
	p-data
	p-sendable
	p-mut
