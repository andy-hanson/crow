no-std
import
	.compare: comparison
	.private.bootstrap: nat
	.private.number-low-level: ==, <=>
export
	.private.bool-low-level: ?, and, bool, false, not, or, true

== bool(a bool, b bool)
	a.to-nat == b.to-nat

<=> comparison(a bool, b bool)
	a.to-nat <=> b.to-nat

# 0 for false, 1 for true.
to-nat nat(b bool) noctx
	b ? 1, 0
