no-std
import
	crow/c-types: c-int, size_t
	crow/private/bootstrap: char8, nat8, const-pointer, mut-pointer

+memcpy nat8 mut*(dest nat8 mut*, src nat8*, size size_t) c extern, pure

+memmove nat8 mut*(dest nat8 mut*, src nat8*, size size_t) c extern, pure

+memset nat8 mut*(begin nat8 mut*, value c-int, size size_t) c extern, pure

+memcmp c-int(s1 nat8*, s2 nat8*, n size_t) c extern, pure

+strlen size_t(a char8 mut*) c extern, pure
