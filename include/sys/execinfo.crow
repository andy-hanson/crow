no-std
import
	crow/c-types: c-int
	crow/private/bootstrap: c-string
	crow/pointer: any-const-pointer, const-pointer, mut-pointer

# Crow interpreter overrides this
backtrace c-int(array any-const-pointer mut*, size c-int) unsafe noctx extern<c>

# Crow interpreter overrides this
backtrace_symbols c-string mut*(array any-const-pointer*, size c-int) unsafe noctx extern<c>
