no-std
import
	crow/extra-types: err-t
	crow/version: is-windows
	crow/ptr: *, mut-ptr

errno err-t() unsafe noctx
	if is-windows
		*_errno
	else
		*__errno_location
._errno err-t mut*() extern<c> noctx unsafe
.__errno_location err-t mut*() extern<c> noctx unsafe

ENOENT err-t() noctx
	2
ESRCH err-t() noctx
	3
EAGAIN err-t() noctx
	11
EWOULDBLOCK err-t() noctx
	EAGAIN
EINVAL err-t() noctx
	22
ETIMEDOUT err-t() noctx
	110
