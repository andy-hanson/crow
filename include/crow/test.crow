no-std
import
	./compare: equal
	./misc: drop, void
	./private/exception-low-level: assert, catch, message, throw
	./str: ==, str

is<a> void(a a, b a) equal<a>
	assert: a == b

throws<a> void(f act a(), message str)
	match f catch
	as ok o
		# TODO: use '_'
		drop: o
		throw: "Expected to throw an exception"
	as err e
		e.message is message
