no-std
import
	./compare: comparison
	./private/number-low-level: ==, <=>, nat64, new
export
	./private/bool-low-level: &&, ||, bool, false, not, true

# Same as `false`.
+new bool() noctx
	false

# Boolean equality.
+== bool(a bool, b bool) noctx
	a.to == b.to

# Compares `false` less than `true`.
+<=> comparison(a bool, b bool) noctx
	a.to <=> b.to

# `0` for `false`, `1` for `true`.
+to nat64(a bool) noctx
	a ? 1
