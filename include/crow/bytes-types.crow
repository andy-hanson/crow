bytes256 record
	by-val
	n0 bytes128
	n1 bytes128

zero bytes16() noctx
	0, 0

zero bytes32() noctx
	zero, zero

zero bytes48() noctx
	zero, zero

zero bytes64() noctx
	zero, zero

zero bytes128() noctx
	zero, zero

zero bytes256() noctx
	zero, zero
