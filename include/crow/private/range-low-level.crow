no-std
import
	../compare: <, compare
	../misc: void

# `low` is inclusive and `high` is exclusive.
range<a> record
	by-val
	# TODO: private constructor
	ignore void
	low a
	high a

.. <a> range<a>(low a, high a) noctx compare<a>
	if low < high
		void, low, high
	else
		# TODO:WARN
		void, high, high

size<a> a(a range a) minus<a>
	a.high - a.low

.minus<a> spec
	- a(a a, b a)
