import
	../exclusion-queue

dummy-exclusion-queue-tests nat()
	0

-eq record nominal
-with-block void(_ eq, cb void mut(queue string exclusion-queue mut*)) unsafe
	queue mut string exclusion-queue = uninit
	a string exclusion-queue mut* = &queue
	a init
	finally a destroy
	cb[a]

test
	trusted with a : eq
		a.dequeue is ()
		a enqueue 0, "a"
		a.dequeue is ((0, "a"),)
		a.dequeue is ()
		a release-exclusion 0

test
	trusted with a : eq
		a.dequeue is ()
		a enqueue 0, "a"
		a.dequeue is ((0, "a"),)
		a.dequeue is ()
		a enqueue 0, "b"
		a.dequeue is ()
		a release-exclusion 0
		a.dequeue is ((0, "b"),)
