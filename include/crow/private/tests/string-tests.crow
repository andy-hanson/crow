import
	crow/math/math: pi

dummy-string-tests nat64()
	0

test
	c8a char8 array = "$¥₿𝄮"
	c8l char8[] = "$¥₿𝄮"
	c32a char32 array = "$¥₿𝄮"
	c32l char32[] = "$¥₿𝄮"
	s string = "$¥₿𝄮"

	c8a.to::string is s
	s.to::(char8 array) is c8a
	c8l.to::string is s
	s.to::char8[] is c8l
	c32a.to::string is s
	s.to::(char32 array) is c32a
	c32l.to::string is s
	s.to::char32[] is c32l

	s.reverse is "𝄮₿¥$"
	"\ue000"::string.to::(char32 array) is "\ue000"
	"\ue000"::(char32 array).to::string is "\ue000"

test
	3::float64.to::string is "3"
	12.3::float64.to::string is "12.3"
	0.04::float64.to::string is "0.04"
	0.123::float64.to::string is "0.123"
	0.001::float64.to::string is "0.001"
	3.14159::float64.to::string is "3.14159"
	pi::float64.to::string is "3.141592653589794"

	infinity::float64.to::string is "Infinity"
	(-infinity)::float64.to::string is "-Infinity"
	(trusted nan)::float64.to::string is "NaN"

test
	"a\nb".lines is ("a", "b")
	"a\r\nb".lines is ("a", "b")
