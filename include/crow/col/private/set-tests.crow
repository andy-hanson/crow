no-std
import
	../../bool: ==, false, true
	../../hash: hash-mix
	../../json: to
	../../misc: void
	../../number: ==, <=>, nat64
	../../string: ==, string, to
	../../test-util: is
	../list: ==, list, new
	../set: ~, ~~, dbg, is-empty, new, set, size, subscript, to

dummy-set-tests nat64()
	0

-verify void(a nat64 set, expected string)
	a.dbg is expected

test
	a nat64 set = ()
	a.is-empty is true
	a.size is 0
	a verify "[]"

	b = a ~ 5
	b.is-empty is false
	b.size is 1
	b.to::nat64[] is (5,)
	b verify "[5]"

	c = b ~~ (6, 4, 1)
	c verify "[1 4 6 5]"

	d = c ~ 8
	d verify "[1 4 6 5], [8]"

	e = d ~~ (4, 10)
	e verify "[1 4 6 5], [10 8]"
	e.size is 6
	e[0] is false
	e[1] is true
	e[2] is false
	e[3] is false
	e[4] is true
	e[5] is true
	e[6] is true
	e[7] is false
	e[8] is true
	e[9] is false
	e[10] is true
	e[11] is false
