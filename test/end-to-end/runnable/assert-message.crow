main void() unsafe
	(() => assert 1::nat + 1 == 3) log-error
	(() => forbid 1::nat + 1 == 2) log-error
	(() => assert false : "Custom assert message") log-error
	(() => forbid true : "Custom forbid message") log-error

log-error void(cb act void()) unsafe
	match cb catch
	as ok ()
		error log "Should throw"
	as error e
		error log "{e}"
