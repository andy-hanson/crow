import
	crow/io/print: out, print

main void() summon
	out print x::e.foo
	out print x::f.foo
	out print x::e.call-foo
	out print x::f.call-foo

e enum
	x
f flags
	x

foo[t] string(a t) t enum-like
	"enum {a}"
foo[t] string(a t) t flags-like
	"flags {a}"

foo[t] spec
	foo string(a t)

call-foo[t] string(a t) t foo
	a foo
