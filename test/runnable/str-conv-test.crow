import
	bootstrap: fut, str
	io.print: print

main fut nat(_ arr str) summon
	assert: "".parse-nat == none
	assert: "1".parse-nat == 1.some
	assert: "1.2".parse-nat == none

	assert: "+1".parse-nat == none

	assert: "123".parse-int == 123.some
	assert: "-123".parse-int == -123.some
	assert: "+123".parse-int == 123.some
	assert: "*123".parse-int == none

	print: "1".parse-nat.to-str
	0 resolved
