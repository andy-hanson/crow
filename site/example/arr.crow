import
	col.arr-util: flatten
	io.print: print

main fut nat(_ arr str) summon
	chili = ["beans", "garlic", "onion", "pepper", "cumin"]
	print: chili[2]

	to-chop = chili[1 -> 4]
	print: to-chop flatten ", "

	hold-the-onion = chili[0 -> 2] ~ chili[3 -> 5]
	print: hold-the-onion flatten ", "

	0 resolved
