import
	crow/io/print: print

main fut nat(_ str[]) summon
	a dinner = "bruschetta", "wine"
	b dinner = "chili", "milk"
	print: a to-str
	print: b to-str
	print: a mix b to-str
	0 resolved

dinner record
	food str
	drink str

mix dinner(a dinner, b dinner)
	a.food, b.drink

to-str str(a dinner)
	"{a food} and {a drink}"
