import
	crow/io/print: print

main fut nat(_ str[]) summon
	a = "bruschetta" dinner "wine"
	b = "chili" dinner "milk"
	print: a to-str
	print: b to-str
	print: a mix b to-str
	0 resolved

dinner record
	food str
	drink str

mix dinner(a dinner, b dinner)
	a.food dinner b.drink

to-str str(a dinner)
	"{a food} and {a drink}"
