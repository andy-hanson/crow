import
	crow.io.print: print

main fut nat(_ arr str) summon
	if n ?= 'bishop'.to-enum<chess-piece>
		print: "bishop is {n to-nat32}"
	if face ?= queen.as<chess-piece>.to-sym.to-enum<face-card>
		print: "queen as face-card is {face to-nat32}"
	0 resolved

chess-piece enum
	pawn
	rook
	knight
	bishop
	queen
	king

face-card enum
	jack
	queen
	king
