import
	collection.arr-util: remove-end, remove-start
	io.print: print
	io.process: exit-code
	path: each-child-recursive

main fut exit-code(_ arr str) summon
	filter = as<act bool(str)>: \x
		drop: x
		true
	"include" each-child-recursive filter, \path
		print: path remove-start "include/" remove-end ".crow"
	0 resolved
