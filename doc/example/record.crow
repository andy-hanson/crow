import
	io.print: print
	io.process: exit-code

dinner record
	food str
	drink str

mix dinner(a dinner, b dinner)
	dinner: a.food, b.drink

to-str str(a dinner)
	a.food + " and " + a.drink

main fut exit-code(_ arr str) summon
	a = dinner: "ramen", "tea"
	b = dinner: "chili", "milk"
	print: a mix b to-str
	0 resolved
