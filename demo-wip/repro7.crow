import
	io.print: print

main fut nat(_ str[]) summon trusted
	p = 0 vec-2-32 0
	c = 0 color-32 0, 0
	v = p vertex c, p
	print: "{v.color.g}"
	()

vertex record
	by-val
	pos vec-2-32
	color color-32
	tex-coord vec-2-32

