import
	io.print: print

main fut nat(_ arr str) summon
	print: "BEGIN"
	x = (1 vec-2-32 2) vertex (3 color-32 4, 5)
	print: "{x.pos.x}"
	print: "{x.pos.y}"
	print: "{x.color.r}"
	print: "{x.color.g}"
	print: "{x.color.b}"
	print: "DONE"
	drop: x
	0 resolved

vertex record
	by-val
	pos vec-2-32
	color color-32

vec-2-32 record
	x float32
	y float32

