import
	crow/io/duration: sec
	crow/io/time: delay
	crow/ptr: any-mut-ptr, null
	pthread: pthread_create, pthread_t

main fut nat(_ str[]) summon unsafe
	thread mut pthread_t = ()
	err = &thread pthread_create null, &thread-fun, null
	assert err == 0
	<- 1.sec delay
	()

.thread-fun any-mut-ptr(a any-mut-ptr) noctx
	assert 1 + 1 == 3
	a
