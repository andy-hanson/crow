import
	crow/io/duration: sec
	crow/io/time: delay
	pthread: pthread_create, pthread_t

main nat^(_ string[]) summon, trusted
	thread mut pthread_t = ()
	err = &thread pthread_create null, &thread-fun, null
	assert err == 0
	<- 1.sec delay
	()

.thread-fun any-mut-pointer(a any-mut-pointer) noctx
	assert 1 + 1 == 3
	a
