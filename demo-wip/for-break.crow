import
	crow/io/print: out, print

main nat^(_ string[]) summon
	out print "hello, world!"
	out print "{(1, 2, 3).find-even ?? 42}"
	out print "{(1, 3) definitely-find-even}"
	()

find-even nat?(a nat[])
	for x : a
		if x is-even
			break x,

definitely-find-even nat(a nat[])
	for x : a
		if x is-even
			break x
		else
			continue
	else
		42
